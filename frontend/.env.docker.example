# Docker Compose Environment
REACT_APP_ENVIRONMENT=docker

# Docker internal networking (backend service name from docker-compose.yml)
REACT_APP_SOCKET_URL=http://backend:8000
REACT_APP_API_URL=http://backend:8000/api/users
REACT_APP_WS_URL=ws://backend:8000

# Note: These URLs use Docker service names, not localhost
# Frontend and backend communicate via Docker network
